<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Insider Web Automation Suite" parallel="false">

    <parameter name="browser" value="firefox"/>

    <listeners>
        <listener class-name="web.listeners.TestListener"/>
    </listeners>

    <test name="Firefox Test">
        <classes>
            <class name="tests.InsiderTest"/>
        </classes>
    </test>
</suite>
